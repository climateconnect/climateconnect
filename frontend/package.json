{
  "name": "climate-connect-frontend",
  "description": "This is the user-facing client code for the Climate Connect platform.",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze-bundle": "ANALYZE=true yarn build",
    "build-storybook": "build-storybook",
    "build": "next build",
    "deploy": "yarn build && yarn export",
    "dev": "next dev",
    "export": "next export",
    "fix-lint": "eslint . --fix",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,md}'",
    "gcp-build": "IS_BUILDING_ON_GOOGLE_CLOUD=1 yarn build",
    "lint:watch": "esw --cache --watch --color --fix",
    "lint": "eslint .",
    "pretest": "yarn lint",
    "start": "NODE_ENV=production node server.js",
    "storybook": "start-storybook -p 6006",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/test-shim.ts",
      "<rootDir>/test/test-setup.ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.ts"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.15",
    "@mui/styles": "^5.11.13",
    "@mui/x-date-pickers": "^6.0.3",
    "@next/bundle-analyzer": "^12.0.7",
    "@react-google-maps/api": "^1.13.0",
    "@sentry/react": "^7.46.0",
    "@types/node": "^18.11.18",
    "@zeit/next-css": "^1.0.1",
    "axios": "^0.21.2",
    "babel-plugin-lodash": "^3.3.4",
    "browser-image-compression": "^1.0.12",
    "cheerio": "^1.0.0-rc.10",
    "date-fns": "^2.12.0",
    "dotenv": "^8.2.0",
    "globby": "^11.0.1",
    "html-react-parser": "^4.0.0",
    "humanize-duration": "^3.22.0",
    "lodash": "^4.17.21",
    "next": "^12.1.0",
    "next-cookies": "^2.0.3",
    "object-path": "^0.11.8",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-avatar-editor": "11.0.7",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^17.0.2",
    "react-ga": "^3.2.0",
    "react-ga4": "^2.0.0",
    "react-google-charts": "^3.0.15",
    "react-infinite-scroller": "^1.2.4",
    "react-linkify": "^1.0.0-alpha",
