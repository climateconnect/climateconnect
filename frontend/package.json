    "babel-loader": "^8.2.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-watch": "^7.0.0",
    "jest": "^26.6.3",
    "next-compose-plugins": "^2.2.0",
    "prettier": "2.2.0"
  }
